@model IEnumerable<BoVoyageBlandineThomasJonathan.Models.DossierReservation>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Créer nouvelle réservation", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Client.Mail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ConseillerClientele.Mail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Participants.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Voyage.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumeroCarteBancaire)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrixTotal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreDeVoyageurs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SolvaviliteCompteBancaire)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssuranceAnnulation)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Client.Mail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConseillerClientele.Mail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Participants.Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Voyage.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumeroCarteBancaire)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrixTotal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreDeVoyageurs)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SolvaviliteCompteBancaire)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssuranceAnnulation)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
