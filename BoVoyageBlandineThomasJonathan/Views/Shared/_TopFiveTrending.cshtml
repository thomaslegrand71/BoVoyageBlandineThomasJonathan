@model IEnumerable<IGrouping<string, BoVoyageBlandineThomasJonathan.Models.Voyage>>


@{
    Layout = null;
}
<div class="row">
    <div class="col-md-3">
        @foreach (var item in Model)
        {
        <table class="table" text-align="center">

            <tr class="row">
                <div class="row" style="width:10cm">
                    Pays : @item.Key  Tarif : @item.ToList()[0].TarifToutCompris €
                </div>
                <div class="row" style="width:10cm">
                    Pays : @item.Key  Tarif : @item.ToList()[1].TarifToutCompris €
                </div>
            </tr>
            

        </table>



        }
    </div>
</div>